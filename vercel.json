{
  "version": 2,
  "builds": [
    {
      "src": "src/main/server/server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main/server/server.py"
    }
  ],
  "functions": {
    "src/main/server/server.py": {
      "includeFiles": "src/drivers/ub_handler.py"
    }
  }
}
